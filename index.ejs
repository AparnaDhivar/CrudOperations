<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

</head>
<body>
  
    <div class="container">
        <div class="row">
            <div class="col">
                <button class="btn btn-success" data-bs-toggle="modal" href="#exampleModalToggle" role="button">new + </button>
            </div>
        </div>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">product</th>
                <th scope="col">qty</th>
                <th scope="col">price</th>
                <th scope="col">action</th>
              </tr>
            </thead>
            <tbody>
                <% data_obj.forEach(function(element){ %>
              <tr>
                <th scope="row">1</th>
                <td><%= element.product_name %></td>
                <td><%= element.pro_qty %></td>
                <td><%= element.pro_price %></td>
                <td><%= "delete" %></td>
         
              </tr>
              <% }); %>
            
            </tbody>
          </table>
    </div>


<!-- model -->
<div class="modal fade" id="exampleModalToggle" aria-hidden="true" aria-labelledby="exampleModalToggleLabel" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalToggleLabel">Modal 1</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            <form method="post" action="/submitdata">
            <div class="form-floating mb-3">
                <input type="text" class="form-control" name="product_name" placeholder="name@example.com">
                <label for="floatingInput">Product</label>
              </div>
              <div class="form-floating mb-3">
                <input type="number" class="form-control" name="pro_qty" placeholder="Password">
                <label for="floatingPassword">Qty</label>
              </div>
              <div class="form-floating mb-3">
                <input type="number" class="form-control" name="pro_price" placeholder="Password">
                <label for="floatingPassword">price</label>
              </div>
              <input type="submit" class="btn btn-primary" value="save">
            </form>
        </div>
      
      </div>
    </div>
  </div>



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>

</body>
</html>